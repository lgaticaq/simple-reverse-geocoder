{
  "name": "simple-reverse-geocoder",
  "version": "1.1.0",
  "description": "Get address from a point",
  "main": "lib",
  "scripts": {
    "prepublish": "npm run build -s",
    "prebuild": "npm run lint -s && npm run clean -s",
    "build": "babel src --out-dir lib --source-maps",
    "lint": "eslint src",
    "clean": "rimraf lib",
    "pretest": "npm run build -s",
    "test": "mocha --compilers js:babel-core/register"
  },
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/simple-reverse-geocoder.git"
  },
  "keywords": [
    "reverse",
    "geocoder"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/simple-reverse-geocoder/issues"
  },
  "homepage": "https://github.com/lgaticaq/simple-reverse-geocoder#readme",
  "dependencies": {
    "bluebird": "^3.3.3",
    "node-geocoder": "^3.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "2.10.1",
    "mocha": "^2.4.5",
    "redis": "^2.4.2",
    "rimraf": "^2.5.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "tonicExampleFilename": "example.js"
}
